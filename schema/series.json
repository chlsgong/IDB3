{
	"id": "int", // response.data.results[i].id
	"title": "string", // response.data.results[i].title
	"description": "string", // response.data.results[i].description
	"startYear": "int", // response.data.results[i].startYear
	"endYear": "int", // response.data.results[i].endYear
	"rating": "string", // response.data.results[i].rating
