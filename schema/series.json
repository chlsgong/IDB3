	"comics": ["int"], // List of ids of comics in this series. Converted from Converted from response.data.results[i].comics
	"numComics": "int", // response.data.results[i].comics.available
	"characters": ["int"], // List of ids of characters this series includes. Converted from response.data.results[i].characters
	"numCharacters": "int", // response.data.results[i].characters.available
	"creators": ["int"], // List of ids of this series' creators. Converted from response.data.results[i].creators
